{
	"name": "Bloc Crew",
	"version": "0.2",
	"manifest_version": 2,
	"description": "Stay up to date with the Bloc Crew!",
    "homepage_url": "http://bloccrew.com/",
    "background": "background.html",
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Bloc Crew",
